# .env.example

# Secret key used to sign and verify JSON Web Tokens (JWT)
# Generate a strong random string for production (e.g. using openssl or a password manager)
JWT_SECRET=your_jwt_secret_here

# MongoDB connection string
# Replace <username>, <password>, <cluster-url>, and <database-name> with your actual details
MONGO_URI=mongodb+srv://<username>:<password>@<cluster-url>/<database-name>?retryWrites=true&w=majority

